{
  "name": "@appland/appmap-agent-js",
  "version": "1.0.1",
  "scripts": {
    "build": "npx rollup lib/server/inline.mjs --file dist/inline.js --format cjs",
    "prettify": "npx prettier --write 'lib/**/*.{js,mjs}' 'test/**/*.mjs' 'bin/*.mjs'",
    "commit": "npx cz",
    "lint": "npx eslint 'lib/**/*.{js,mjs}' 'test/**/*.mjs' 'bin/*.mjs'",
    "test": "node test/unit/run.mjs",
    "deploy": "npx semantic-release"
  },
  "files": [
    "src/**/*.js",
    "lib/**/*.mjs",
    "bin/*.mjs"
  ],
  "bin": {
    "appmap": "bin/agent.mjs"
  },
  "dependencies": {
    "acorn": "^7.4.1",
    "escodegen": "^2.0.0",
    "minimist": "^1.2.5",
    "npm": "^6.14.12",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "c8": "^7.6.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/applandinc/appmap-agent-js.git"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
