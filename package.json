{
  "name": "@appland/appmap-agent-js",
  "version": "7.1.2",
  "bin": {
    "appmap-agent-js": "bin/bin.mjs"
  },
  "main": "lib/node/recorder-manual.mjs",
  "os": [
    "linux",
    "darwin"
  ],
  "engine": {
    "node": ">=12.0.0 <13.0.0 || >= 14.0.0"
  },
  "licence": "MIT",
  "keywords": [
    "appmap",
    "appland",
    "javascript",
    "instrumentation",
    "visualization",
    "tracing",
    "recording",
    "testing"
  ],
  "homepage": "https://github.com/applandinc/appmap-agent-js.git/#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/applandinc/appmap-agent-js.git"
  },
  "scripts": {
    "tt": "npx test-turtle --layout alongside 'npx eslint $1 $2 && npx prettier -w $1 $2 && node --unhandled-rejections=strict $2 && npx c8 --include $1 --check-coverage --branches 100 --functions 100 -- node --unhandled-rejections=strict $2'",
    "lint": "npx eslint '**/*.@(mjs|js|cjs)'",
    "format": "npx prettier -w '**/*.@(mjs|js|cjs)'",
    "build": "node build/index.mjs",
    "test-unit-raw": "for test in $(find build components -name '*.test.mjs'); do echo \"$test ...\" && (node \"$test\" || echo \"> Failure $?\"); done",
    "commit": "npx cz",
    "test-unit": "npx test-turtle --layout alongside 'npx c8 --include $1 --check-coverage --branches 100 --functions 100 -- node --unhandled-rejections=strict $2'",
    "test-integration": "node --unhandled-rejections=strict test/index.mjs",
    "deploy": "npx semantic-release"
  },
  "dependencies": {
    "@appland/appmap-validate": "^2.0.0",
    "@babel/parser": "^7.15.8",
    "acorn": "^8.5.0",
    "ajv": "^8.6.3",
    "ajv-error-tree": "0.0.5",
    "escodegen": "^2.0.0",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "net-socket-messaging": "^0.1.6",
    "posix-socket": "^1.1.1",
    "posix-socket-messaging": "0.0.1",
    "prompts": "^2.4.2",
    "shell-quote": "^1.7.3",
    "source-map": "^0.6.1",
    "treeify": "^1.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "c8": "^7.6.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "jsdom": "^16.6.0",
    "mocha": "^8.4.0",
    "mysql": "^2.18.1",
    "pg": "^8.6.0",
    "prettier": "^2.2.1",
    "semantic-release": "^18.0.0",
    "sqlite3": "^5.0.2",
    "tap": "^15.0.10",
    "tape": "^5.2.2",
    "test-turtle": "^4.0.2"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
